/* pages/profile/component/ListView/list-view.wxss */
.list-view{
  border-top: 36rpx solid #f8f8f8;
}

.list-view .list-item{
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 30rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #f8f8f8;
}

.list-view .list-item .image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}

.list-view .list-item .text{
  margin-left: 16rpx;
}
