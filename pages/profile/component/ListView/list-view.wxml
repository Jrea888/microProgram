<!--pages/profile/component/ListView/list-view.wxml-->
<view class="list-view">
  <block wx:for="{{infos}}" wx:key="index">
    <view class="list-item">
      <image class="image" src="/assets/images/profile/{{item.icon}}"></image>
      <text class="text">{{item.info}}</text>
    </view>
  </block>
</view>

